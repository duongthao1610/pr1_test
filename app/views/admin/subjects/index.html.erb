<% provide :title, "Manage subject" %>
<div class="div-add-new">
  <%= link_to "Add New subject", new_admin_subject_path %>
</div>
<div class="col-sm-12 no-padding-right">
  <table class="table table-hover bg-table ls-result text-center">
    <thead class="title_table">
    <tr>
      <th class="text-center tbl-cate-name"><%= "Name" %></th>
      <th class="text-center cate-description"><%= "Description" %></th>
      <th class="text-center create-date"><%= "Create" %></th>
      <th class="text-center opt-edit"><%= "Edit" %></th>
      <th class="text-center opt-delete"><%= "Delete" %></th>
    </tr>
    </thead>
    <tbody>
    <%= render partial: "subject", collection: @subjects, as: :subject %>
    </tbody>
  </table>
<!--  <div class="wrap-paging fix-paging"><%#= will_paginate %></div>-->
</div>